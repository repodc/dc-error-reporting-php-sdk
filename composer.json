{
    "name": "dc-tec/dc-error-reporting-php-sdk",
    "description": "SDK para integrar o sistema de alerta de erros com PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vinicius Vidal",
            "email": "vidal@tecnologiadc.com.br"
        }
    ],
    "require": {
        "php": ">=7.2.0"
    },
    "autoload": {
        "psr-4": {
            "DcTec\\DcErrorReportingPhpSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DcTec\\DcErrorReportingPhpSdk\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/repodc/dc-error-reporting-php-sdk.git"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
